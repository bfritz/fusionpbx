<include>
  <!-- Client Profile (Original mode) -->
  <!-- to use this profile take the x- away from the open and close tags so its <profile> and </profile> -->
  <profile type="client">
    <param name="name" value="$${xmpp_client_profile}"/>
    <param name="login" value="<!--username-->/talk"/>
    <param name="password" value="<!--password-->"/>
    <param name="dialplan" value="XML"/>
    <param name="context" value="<!--domain-->"/>
    <param name="message" value="Jingle all the way"/>
    <param name="rtp-ip" value="<!--rtpip-->"/>
    <!-- <param name="ext-rtp-ip" value="auto-nat"/> -->
    <param name="auto-login" value="<!--autologin-->"/>
    <!-- SASL "plain" or "md5" -->
    <param name="sasl" value="<!--sasl_type-->"/>
    <!-- if the server where the jabber is hosted is not the same as the one in the jid -->
    <param name="server" value="<!--xmpp_server-->"/>
    <!-- Enable TLS or not -->
    <param name="tls" value="<!--tls_enable-->"/>
    <!-- disable to trade async for more calls -->
    <param name="use-rtp-timer" value="<!--use_rtp_timer-->"/>
    <!-- default extension (if one cannot be determined) -->
    <param name="exten" value="<!--default_exten-->"/>
    <!-- VAD choose one -->
    <!-- <param name="vad" value="in"/> -->
    <!-- <param name="vad" value="out"/> -->
    <!--<param name="vad" value="both"/>-->
    <!--<param name="avatar" value="/path/to/tiny.jpg"/>-->
    <!--<param name="candidate-acl" value="wan.auto"/>-->
    <param name="local-network-acl" value="localnet.auto"/>
  </profile>
</include>
