#!/bin/sh
set -e

case "$1" in
  configure)
    for x in \
      /usr/share/nginx/www/fusionpbx/app/modules
    do
      chown -R www-data:www-data $x
    done
    ;;
  abort-upgrade|abort-remove|abort-deconfigure)
    ;;
  *)
    echo "postinst called with unknown argument \`$1'" >&2
    exit 1
    ;;
esac

#DEBHELPER#
exit 0
